    font-size: 2.4em;
    text-transform: uppercase;
}
h3 {
    font-size: 30px;
    text-transform: uppercase;
}
h4 {
    font-size: 1.8em;
    text-transform: uppercase;
}
h5 {
    font-size: 21px;
}
h6 {
    /*font-size: 1em;*/
