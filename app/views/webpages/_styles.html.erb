body{
    font: 300 18px/30px "Open Sans",arial,sans-serif;
}

p{
    font-size: 16px;
}

h1, h2, h3, h4, h5, h6 {
    color: #272727;
    font-family: 'Montserrat',sans-serif;
    font-weight: 400;
    line-height: 1.17em;
    /*margin: 0 0 20px;*/
}
h1 {
    font-size: 3.5em;
    text-transform: uppercase;
}
