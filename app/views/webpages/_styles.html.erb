/* Navbar 1
======================================== */
.navbar-1.navbar-default {
    background-color: #fff;
    letter-spacing: 1.5px;
    padding: 10px 0;
}

.navbar-1.navbar-default .navbar-nav > li > a {
font-family: 'montserrat', sans-serif;
font-size: 11px;
text-transform: uppercase;
color: #58687a;
}


.navbar-1.navbar-default .navbar-brand {
    color: #58687a;
    font-family: 'montserrat';
    font-size: 20px;
    font-weight: 700;
}

.navbar-1.navbar-default .navbar-right .fa {
color: #58687a;
font-size: 20px;
}

.navbar-1.navbar-default .navbar-nav > li > a:hover,
.navbar-1.navbar-default .navbar-nav > li > a:focus {
    color: #C61D38;
    -webkit-transition: color .2s ease-in-out;
    -moz-transition: color .2s ease-in-out;
    -ms-transition: color .2s ease-in-out;
    -o-transition: color .2s ease-in-out;
    transition: color .2s ease-in-out;
}


/* Navbar 2
======================================== */
.navbar-2.navbar-default {
    background-color: #222;
    letter-spacing: 1.5px;
