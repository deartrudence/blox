<%= form_for(@webpage) do |f| %>


  <div class="field">
    <%= f.text_field :name, class: "webpage-name" %>
  </div>


  <div class="actions">
    <%= f.submit 'Update', :class => "btn btn-primary" %>
  </div>
<% end %>

<%= link_to "Generate Code", webpage_path(@webpage), class: "btn btn-primary" %>
