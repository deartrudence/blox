


      <strong><%= category.name %></strong>
      <ul>
        <% for bloc in category.blocs %>
          <li><%= bloc.name %>
              <% if @webpage.present? %>
              <%= link_to "+", webpage_path(:webpage => @webpage, :bloc => bloc), method: :put %></li>
              <% end %>
        <% end %>
      </ul>
  